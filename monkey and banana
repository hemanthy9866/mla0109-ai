monkey_pos = "door"
box_pos = "window"
banana_pos = "middle"
monkey_on_box = False
def move_monkey(place):
    global monkey_pos
    print(f"Monkey moves to {place}")
    monkey_pos = place
def push_box(place):
    global box_pos
    print(f"Monkey pushes the box to {place}")
    box_pos = place
def climb_box():
    global monkey_on_box
    print("Monkey climbs on the box")
    monkey_on_box = True
def get_bananas():
    if monkey_on_box and box_pos == banana_pos:
        print("Monkey grabs the bananas! üêíüçå")
    else:
        print("Monkey cannot reach the bananas yet.")
move_monkey("window")     
push_box("middle")        
move_monkey("middle")     
climb_box()               
get_bananas()
